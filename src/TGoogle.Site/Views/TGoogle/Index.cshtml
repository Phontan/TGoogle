@model dynamic

<!DOCTYPE html>

<html>
    <head>
        <title>title</title>
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js" type="text/javascript"></script>
    </head>
    <body>
        <script type="text/javascript">
            $(function () {
                $("#getResult").click(function () {
                    var keyWord = $("#keyWord")[0].value;
                    $.ajax({
                        url: "/TGoogle/Search/",
                        type: 'GET',
                        data: "keyWord="+keyWord,
                        success: showResult,
                        error: function() {
                            alert('error');
                        }
                    });
                });
                
                function showResult(data) {
                    if (data.responseStatus != 200)
                        return;
                    $("#resultList li").remove();
                    $.each(data.responseData.results, function () {
                        $("<li><div><h3><a href=\"" + this.url + "\">" + this.title + "</a></h3><div>" + this.url + "<br>" + this.content + "</div></div></li>").appendTo("#resultList");
                    });

                }
            })
        </script>
        
        <div>
            <input type="text" id="keyWord" /><br />
            <input type="button" id="getResult" value="Search" />
            <div id="resultView">
                <ol id="resultList"></ol>
            </div>
        </div>
    </body>
</html>